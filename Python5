tom = Turtle()
tom.shape('turtle')
tom.color('green')
tom.speed(100)

tom.penup()
tom.goto(-470,-150)
tom.pendown()

def rectangle(color,height):
    tom.color(color)
    tom.begin_fill()
    tom.fd(940)
    tom.lt(90)
    tom.fd(height)
    tom.lt(90)
    tom.fd(940)
    tom.lt(90)
    tom.fd(height)
    tom.fd(-height)
    tom.lt(90)
    tom.end_fill()

rectangle("sienna",30)
rectangle("forestgreen",30)
rectangle("palegreen",150)
rectangle("gray",15)
rectangle("dimgrey",60)
rectangle("gray",15)

tom.penup()
tom.rt(90)
tom.fd(50)
tom.lt(90)
tom.pendown()

tom.color("white")
tom.begin_fill()
tom.fd(20)
tom.lt(90)
tom.fd(10)
tom.lt(90)
tom.fd(20)
tom.lt(90)
tom.fd(10)
tom.lt(90)
tom.end_fill()

tom.penup()
tom.fd(60)
tom.pendown()

for loop in range(10):
    tom.begin_fill()
    tom.fd(50)
    tom.lt(90)
    tom.fd(10)
    tom.lt(90)
    tom.fd(50)
    tom.lt(90)
    tom.fd(10)
    tom.lt(90)
    tom.end_fill()
    tom.penup()
    tom.fd(90)
    tom.pendown()
    
tom.penup()
tom.goto(-470,-90)

tom.lt(90)
tom.fd(50)
tom.rt(90)
tom.fd(100)
tom.pendown()
tom.color("sienna")
tom.begin_fill()
tom.fd(20)
tom.lt(90)
tom.fd(60)
tom.lt(90)
tom.fd(20)
tom.lt(90)
tom.fd(60)
tom.end_fill()
tom.fd(-60)
tom.lt(90)
tom.color("darkgreen")
tom.fd(-26.4)
tom.begin_fill()
tom.fd(72.8)
tom.lt(110)
tom.fd(106.4)
tom.lt(140)
tom.fd(106.4)
tom.end_fill()
tom.fd(-58.5)
tom.rt(70)
tom.color("mediumseagreen")
tom.begin_fill()
tom.fd(5.46)
tom.rt(110)
tom.fd(63.9)
tom.rt(140)
tom.fd(63.9)
tom.rt(110)
tom.fd(43.7)
tom.end_fill()

